{
  "compilerOptions": {
    "preserveConstEnums": true,
    "strictNullChecks": true,
    "inlineSources": true,
    "inlineSourceMap": true,
    "sourceRoot": "/",
    "target": "es2019",
    "outDir": ".build",
    "module": "es2020",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "lib": [
      "es2019"
    ],
    "rootDir": "./src",
    "resolveJsonModule": true
  },
  "exclude": [
    "test"
  ]
}