{
  "name": "micro-lambda-ts",
  "version": "1.0.0",
  "description": "Microservice template in Typescript",
  "main": "index.ts",
  "scripts": {
    "build:lint": "eslint src/ test/ --ext .ts",
    "build:clean": "rimraf dist/",
    "build:compile": "serverless webpack",
    "build": "npm run build:lint && npm run build:clean && npm run build:compile",
    "package": "serverless package",
    "deploy": "serverless deploy",
    "deploy:tst": "npm run deploy -- --aws-profile default --stage tst",
    "start": "serverless offline start --stage tst",
    "test:unit": "nyc mocha -r ts-node/register 'test/unit/**/*.ts'",
    "test:integration": "mocha -r ts-node/register  -r dotenv/config 'test/integration/**/*.ts' --timeout 32000",
    "test": "npm run build:lint && npm run test:unit"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "aws-sdk": "^2.891.0",
    "aws-serverless-express": "^3.4.0",
    "cors": "^2.8.5",
    "dayjs": "^1.10.4",
    "express": "^4.17.1",
    "got": "^11.8.2",
    "swagger-jsdoc": "^7.0.0-rc.6",
    "swagger-ui-dist": "^3.47.1",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "-": "0.0.1",
    "@babel/core": "^7.13.8",
    "@babel/preset-env": "^7.13.9",
    "@babel/preset-typescript": "^7.13.0",
    "@types/aws-serverless-express": "^3.3.3",
    "@types/chai": "^4.2.16",
    "@types/chai-as-promised": "^7.1.3",
    "@types/cors": "^2.8.10",
    "@types/express": "^4.17.11",
    "@types/mocha": "^8.2.2",
    "@types/node": "^14.14.33",
    "@types/sinon": "^10.0.0",
    "@types/sinon-chai": "^3.2.5",
    "@types/supertest": "^2.0.11",
    "@types/swagger-jsdoc": "^6.0.1",
    "@types/swagger-ui-dist": "^3.30.1",
    "@types/uuid": "^8.3.1",
    "@typescript-eslint/eslint-plugin": "^4.17.0",
    "@typescript-eslint/parser": "^4.17.0",
    "chai": "^4.3.4",
    "chai-as-promised": "^7.1.1",
    "eslint": "^7.21.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-plugin-import": "^2.22.1",
    "mocha": "^8.3.2",
    "nyc": "^15.1.0",
    "serverless": "^2.29.0",
    "serverless-domain-manager": "^5.1.0",
    "serverless-offline": "^6.8.0",
    "serverless-plugin-log-retention": "^2.0.0",
    "serverless-pseudo-parameters": "^2.5.0",
    "serverless-webpack": "^5.3.5",
    "sinon": "^10.0.0",
    "sinon-chai": "^3.6.0",
    "supertest": "^6.1.3",
    "ts-loader": "^8.0.17",
    "ts-mockito": "^2.6.1",
    "ts-node": "^10.2.0",
    "typescript": "^4.3.5",
    "webpack": "^4.46.0",
    "webpack-node-externals": "^2.5.2"
  }
}
